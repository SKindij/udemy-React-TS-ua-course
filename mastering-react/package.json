{
  "name": "mastering-react",
  "description": "Udemy course: web-dev with React",
  "private": true,
  "version": "0.2.6",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "watch-errors": "tsc --watch",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "storybook": "start-storybook -p 6006",
    "storybook:node18": "NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "check-deps": "npx npm-check"
  },
  "dependencies": {
    "@rematch/core": "^2.2.0",
    "@tanstack/react-query": "^4.32.6",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@storybook/addon-actions": "^7.3.0",
    "@storybook/addon-essentials": "^7.3.0",
    "@storybook/addon-interactions": "^7.3.0",
    "@storybook/addon-links": "^7.3.0",
    "@storybook/builder-vite": "^7.2.3",
    "@storybook/react": "^7.2.3",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/jest-dom": "^6.0.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/lodash": "^4.14.197",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.34.2",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  }
}
